{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "skipLibCheck": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react"
    },
    "include": [
      "src/**/*.tsx",
      "./shell/**/*.tsx", "shell/main.js"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
}
